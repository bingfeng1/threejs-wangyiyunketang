{
    "compilerOptions": {
        "types": [
            "element-plus/global"
        ]
    },
    "vueCompilerOptions": {
        "experimentalDisableTemplateSupport": true
    },
    "paths": {
        "@/*": [
            "./src/*"
        ],
        "comps/*": [
            "./src/components/*"
        ],
        "apis/*": [
            "./src/apis/*"
        ],
        "pages/*": [
            "./src/pages/*"
        ],
        "utils/*": [
            "./src/utils/*"
        ],
        "routes/*": [
            "./src/routes/*"
        ],
        "styles/*": [
            "./src/styles/*"
        ]
    }
}